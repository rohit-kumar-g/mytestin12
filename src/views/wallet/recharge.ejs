                                        window.location.href = "/wallet/recharge/pay";
                                    } else if (selectedType === "MOMO") {
                                        window.location.href = "/wallet/recharge/pay1";
                                    } else if (selectedType === "bank2") {
                                        window.location.href = "/wallet/recharge/pay2";
                                    } else if (selectedType === "usdt") {
                                        window.location.href = "/wallet/recharge/pay3";
                                    } else if (selectedType === "trx") {
                                        window.location.href = "/wallet/recharge/pay4";
                                    }
                                }
                            }, 1500);
                        } else {
                            // Handle the case when the order ID is not valid
                            console.error("Invalid order ID");
                        }
                    },
                    error: function (xhr, status, error) {
                        // Handle AJAX request errors
                        console.error("AJAX request failed:", status, error);
                    }
                });
            } else {
                alertMess('Deposit amount range: 250~20000', $(this));
            }
        });

        $('.info .box').click(function (e) {
            let text = $(this).find('.text').text().trim();
            navigator.clipboard.writeText(text);
