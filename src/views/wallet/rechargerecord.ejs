                                    <div data-v-a774625f="" class="recharge_right recharge"> <span data-v-3966082f="" style="color: ${(data.status == 1) ? 'rgb(106, 190, 87)' : 'rgb(242, 65, 59)'}"> ${(data.status == 0) ? 'Waiting' : (data.status == 1) ? 'Success' : 'Canceled'}</span>
                                    </div>
                                </div>
                                <div data-v-a774625f="" role="separator"
                                    class="van-divider van-divider--hairline divier">
                                    <!---->
                                </div>
                                <div data-v-a774625f="" class="rechargeh__container-content__item-body">
                                    <div data-v-a774625f=""><span data-v-a774625f="">Balance</span><span
                                            data-v-a774625f="" class="price"> ${data.money}.00 â‚¹</span></div>
                                    <div data-v-a774625f=""><span data-v-a774625f="">Type</span><span
                                            data-v-a774625f="">${data.type}</span></div>
                                    <div data-v-a774625f=""><span data-v-a774625f="">Time</span><span
                                            data-v-a774625f="" class"time"">${timerJoin(data.time)}</span></div>
                                    <div data-v-a774625f=""><span data-v-a774625f="">Order number</span>
                                        <div data-v-a774625f="" class="order number" data-clipboard-text="${data.id_order}"><span
                                                data-v-a774625f=""> ${data.id_order}</span><img data-v-a774625f=""
                                                src="data:image/png;base64,"
                                                alt=""></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    
                    
                    `;
            })
            $('.van-list .lisys').html(html);
            $('.number').click(function (e) {
                e.preventDefault();
                let text = $(this).text().trim();
                navigator.clipboard.writeText(text);
                $(this).addClass('block-click');
                alertMess('Copy successful', $(this));
            });
        }
        function alertMess(text, sic) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" sstyle=""> ${text} </div>
                 </div>
                 `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, 1000);
        }
        $.ajax({
            type: "GET",
            url: "/api/webapi/recharge/list",
            dataType: "json",
            success: function (response) {
                console.log(response.datas);
                if (response.datas.length == 0) return;
                loads(response.datas);
            }
        });
        // Get the draggable element
    var draggableElement = document.getElementById('customerId');
    
    // Variables to store mouse position
    var offsetX = 0;
    var offsetY = 0;

    // Function to handle touch start event
    function handleTouchStart(e) {
        offsetX = e.touches[0].clientX - draggableElement.getBoundingClientRect().left;
        offsetY = e.touches[0].clientY - draggableElement.getBoundingClientRect().top;
    }

    // Function to handle touch move event
    function handleTouchMove(e) {
        e.preventDefault();
        var x = e.touches[0].clientX - offsetX;
        var y = e.touches[0].clientY - offsetY;
        
        // Update the position of the draggable element
        draggableElement.style.left = x + 'px';
        draggableElement.style.top = y + 'px';
    }

    // Add event listeners for touch events
    draggableElement.addEventListener('touchstart', handleTouchStart, false);
    draggableElement.addEventListener('touchmove', handleTouchMove, false);
    </script>
